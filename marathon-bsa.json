{
  "id": "/marathon-bsa",
  "cmd": "LIBPROCESS_PORT=$PORT1 && ./bin/start     --checkpoint --decline_offer_duration \"120000\" --default_accepted_resource_roles \"*\"  --enable_features \"vips,task_killing\"  --event_stream_max_outstanding_messages \"50\"  --executor \"//cmd\" --failover_timeout \"604800\" --framework_name marathon-bsa  --ha --hostname $LIBPROCESS_IP    --http_event_callback_slow_consumer_timeout \"10000\"  --http_port  $PORT0  --http_realm \"Mesosphere\"   --launch_token_refresh_interval \"30000\" --launch_tokens \"100\" --leader_proxy_connection_timeout \"5000\" --leader_proxy_read_timeout \"10000\" --local_port_max \"20000\" --local_port_min \"10000\"   --master \"zk://master.mesos:2181/mesos\"  --max_tasks_per_offer \"1\" --mesos_authentication_principal marathon-bsa  --mesos_leader_ui_url \"/mesos\" --mesos_role marathon-bsa   --metrics --min_revive_offers_interval \"5000\" --offer_matching_timeout \"1000\" --on_elected_prepare_timeout \"180000\"   --reconciliation_initial_delay \"15000\" --reconciliation_interval \"600000\"   --revive_offers_repetitions \"3\" --save_tasks_to_launch_timeout \"3000\" --scale_apps_initial_delay \"15000\" --scale_apps_interval \"300000\"    --store_cache --task_launch_confirm_timeout \"300000\" --task_launch_timeout \"300000\" --task_reservation_timeout \"20000\" --disable_tracing  --zk zk://master.mesos:2181/universe/marathon-bsa  --zk_compression --zk_compression_threshold \"65536\" --zk_max_versions \"25\" --zk_session_timeout \"10000\" --zk_timeout \"10000\"",
